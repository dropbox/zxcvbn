{
    "name": "zxcvbn",
    "version": "1.1.0-unstable",
    "description": "realistic password strength estimation",
    "main": "index.js",
    "scripts": {
        "build": "npm run build-zxcvbn && npm run build-demo",
        "build-zxcvbn": "./build/compile_and_minify.sh",
        "build-demo": "./build/build_demo.sh",
